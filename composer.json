{
  "name": "rostenkowski/doctrine",
  "type": "library",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "prefer-dist": true,
  "require": {
    "php": ">=7.1",
    "ext-mbstring": "*",
    "ext-tokenizer": "*",
    "ext-pdo": "*",
    "nette/di": "^3",
    "nette/utils": "^3",
    "doctrine/orm": "^2",
    "symfony/console": "^3"
  },
  "require-dev": {
    "nette/tester": "^2",
    "tracy/tracy": "^2"
  },
  "autoload": {
    "psr-4": {
      "Rostenkowski\\Doctrine\\": [
        "src/Doctrine",
        "tests",
        "tests/entities",
        "tests/queries"
      ]
    }
  }
}
